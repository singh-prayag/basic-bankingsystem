<h1 >
    Here are transaction History Records
  </h1>
  
  <table class="table table-hover">
    <thead>
      <tr>
        <th scope="col">S.no</th>
        <th scope="col">Sender</th>
        <th scope="col">Receiver</th>
        <th scope="col">Amount</th>
        <th scope="col">Date & Time</th>
      </tr>
    </thead>
    <tbody>
      <% for(var i=0;i<tdata.length;i++){ %>
        <tr>
          <th scope="col">
            <%=i+1%>
          </th>
          <th scope="col">
            <%=tdata[i].dUser %>
          </th>
          <th scope="col">
            <%=tdata[i].cUser %>
          </th>
          <th scope="col">
            <%=tdata[i].tamount %>
          </th>
          <td scope="col">
            <%=tdata[i].time %>
          </td>
        </tr>
        <% } %>
    </tbody>
  </table>
  </div>